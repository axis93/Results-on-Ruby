<h1>New Result</h1>
<%= form_with scope: :result, url: results_path, local: true do |form| %>
<h2>Add a result:</h2>
<div id="add_result">
<p>
	<%= form.label :name %>
	<%= form.text_field :name1 %>
</p>

<p>
	<%= form.label :goal %>
	<%= form.text_field :goal1 %>
</p>

<p>
	<%= form.label :name %>
	<%= form.text_field :name2 %>
</p>

<p>
	<%= form.label :goal %>
	<%= form.text_field :goal2 %>
</p>
<p>
	<%= form.label :date %>
	<%= form.date_field :date %>
</p>

<p>
		<%= form.submit %>
</p>
</div>
<% end %>

<%= link_to 'Back', results_path %>